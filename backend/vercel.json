{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/photos/delete-photo",
      "dest": "src/controllers/deletePhotoInTrash.mjs"
    }
  ],
  "crons": [
    {
      "path": "/photos/delete-photo",
      "schedule": "5 * * * *"
    }
  ]
}
